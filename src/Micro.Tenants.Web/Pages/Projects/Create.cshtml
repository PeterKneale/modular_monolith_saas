@page "/org/{org}/projects/create"
@{
    ViewData["Title"] = "Create Project";
    ViewData["PageId"] = "ProjectCreate";
}
@model Create

<div class="card mb-2">
    <div class="card-header">
        <h5>Create a new project</h5>
    </div>
    <div class="card-body">
        <partial name="_ValidationSummary"/>
        <form method="post" class="card">
            <div class="card-body">
                @Html.EditorFor(x => x.Name, null, new { autofocus = "autofocus" })
            </div>
            <div class="card-footer">
                <input type="submit" value="Create" class="btn btn-primary ms-auto" data-testid="Create"/>
            </div>
        </form>
    </div>
</div>