@page
@model CreatePage

<partial name="_ValidationSummary"/>

<form method="post" class="card">
    <div class="card-body">
        @Html.EditorFor(x => x.Name, null, new { autofocus = "autofocus" })
    </div>
    <div class="card-footer">
        <input type="submit" value="Add" class="btn btn-primary ms-auto" data-testid="Add"/>
    </div>
</form>